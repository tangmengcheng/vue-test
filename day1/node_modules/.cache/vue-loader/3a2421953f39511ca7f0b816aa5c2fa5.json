 // 父调用子组件\n    },\n    provide() {\n        return {\n            parent: this\n        }\n    },\n    data() {\n        return {\n            money: 100,\n            isSmoke: '是否抽烟'\n        }\n    },\n    methods: {\n        // change(val) {\n        //     this.money = val;\n        // },\n        son() {\n            console.log('有人点我')\n        }\n    },\n    components: {\n        Son1,\n        Son2\n    }\n}\n</script>"]}]}